<h2>Date Dimension</h2>

<%= pagination_links @dimension_pages %>
<table class="report">
  <tr>
    <%  DateDimension.columns.each do |column| -%>
    <th><%= column.name.humanize.titleize %></th>
    <%  end -%>
  </tr>
<%  @dimension_records.each do |record| -%>
  <tr>
    <%  DateDimension.columns.each do |column| -%>
    <td><%= record.send(column.name.to_sym) %></td>
    <%  end -%>
  </tr>
<%  end -%>
</table>
<%= pagination_links @dimension_pages %>